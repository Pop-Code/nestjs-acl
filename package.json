{
    "name": "nestjs-acl",
    "version": "0.0.1",
    "description": "A nestjs package to check acl rules at runtime",
    "author": "Rmannn <alex.hermann@pop-code.com>",
    "repository": "https://github.com/POP-CODE/nestjs-acl.git",
    "license": "UNLICENSED",
    "publishConfig": {
        "access": "restricted"
    },
    "main": "lib/index.js",
    "scripts": {
        "build": "rm -Rf lib && tsc -b",
        "format": "prettier --write \"src/**/*.{ts,tsx}\" --parser typescript",
        "doc": "rm -Rf ./docs && typedoc ./src && touch ./docs/.nojekyll",
        "test": "jest",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "lint": "tslint -c tslint.json -t verbose 'src/**/*.{ts,tsx,js,jsx}'"
    },
    "peerDependencies": {
        "@nestjs/common": "~6"
    },
    "dependencies": {
        "debug": "4.1.1",
        "lodash": "4.17.15",
        "accesscontrol": "2.2.1",
        "nest-access-control": "2.0.1"
    },
    "devDependencies": {
        "@nestjs/core": "6.10.13",
        "@nestjs/common": "6.10.13",
        "@nestjs/testing": "6.10.13",
        "@types/node": "13.1.2",
        "reflect-metadata": "0.1.13",
        "rxjs": "6.5.4",
        "jest": "24.9.0",
        "ts-jest": "24.2.0",
        "ts-node": "8.5.4",
        "tsconfig-paths": "3.9.0",
        "typedoc": "0.15.6",
        "typescript": "3.7.4",
        "prettier": "1.19.1",
        "tslint": "5.20.1",
        "@types/jest": "24.0.25",
        "@types/debug": "4.1.5"
    }
}
