{
    "name": "nestjs-acl",
    "version": "0.1.0",
    "description": "A nestjs package to check acl rules at runtime",
    "author": "Rmannn <alex.hermann@pop-code.com>",
    "repository": "https://github.com/POP-CODE/nestjs-acl.git",
    "license": "MIT",
    "main": "dist/index.js",
    "scripts": {
        "build": "rm -Rf dist && tsc -b",
        "format": "prettier \"**/*.ts\" --ignore-path ./.prettierignore --write && git status",
        "doc": "rm -Rf ./docs && typedoc ./src && touch ./docs/.nojekyll",
        "lint": "eslint src/**/**.ts",
        "test": "jest",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
    },
    "peerDependencies": {
        "@nestjs/common": "^6 || ^7.0.0"
    },
    "dependencies": {
        "debug": "4.1.1",
        "lodash": "4.17.15",
        "accesscontrol": "2.2.1",
        "nest-access-control": "2.0.1"
    },
    "devDependencies": {
        "@nestjs/core": "7.0.8",
        "@nestjs/common": "7.0.8",
        "@nestjs/testing": "7.0.8",
        "@types/node": "13.13.2",
        "reflect-metadata": "0.1.13",
        "rxjs": "6.5.5",
        "jest": "25.4.0",
        "ts-jest": "25.4.0",
        "ts-node": "8.9.0",
        "tsconfig-paths": "3.9.0",
        "typedoc": "0.17.4",
        "typescript": "3.8.3",
        "prettier": "2.0.5",
        "eslint": "6.8.0",
        "eslint-plugin-import": "2.20.2",
        "eslint-plugin-prefer-arrow": "1.2.0",
        "@typescript-eslint/eslint-plugin": "2.29.0",
        "@typescript-eslint/parser": "2.29.0",
        "@types/jest": "25.2.1",
        "@types/debug": "4.1.5",
        "eslint-config-prettier": "6.11.0"
    }
}
